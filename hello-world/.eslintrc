{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-indent": [2, 2],
        "react/jsx-indent-props": [2, 2]
    },
    "plugins": [
        "react"
    ]
}